    if (index == 2) {\n      this.props.history.replace(\"/hotel\");\n    }\n    if (index == 3) {\n      this.props.history.replace(\"/trainTickets\");\n    }\n    if (index == 4) {\n      this.props.history.replace(\"/holiday\");\n    }\n    if (index == 5) {\n      this.props.history.replace(\"/groupBuying\");\n    }\n    if (index == 6) {\n      this.props.history.replace(\"/tickets\");\n    }\n    if (index == 7) {\n      this.props.history.replace(\"/local\");\n    }\n  }\n  render() {\n    return (\n      <div className=\"home\">\n        <Head />\n        {this.state.componentNum == 0 ? (\n          <div className=\"goWhereInput\">\n            <div className=\"leftImg\">\n              <Image\n                style={{ width: 100, height: 40 }}\n                src=\"https://gw.alipayobjects.com/zos/antfincdn/aPkFc8Sj7n/method-draw-image.svg\"\n              />\n            </div>\n            <div className=\"input\">\n              <Search\n                placeholder=\"搜索目的地\"\n                onSearch={this.onSearch}\n                enterButton\n              />\n            </div>\n            <div className=\"telePeople\">\n              <Image\n                style={{ width: 100, height: 40 }}\n                className=\"img\"\n                src=\"https://gw.alipayobjects.com/zos/antfincdn/aPkFc8Sj7n/method-draw-image.svg\"\n              />\n              <div className=\"content\">\n                <p>\n                  <span>国内客服</span>95117\n                </p>\n                <p>\n                  <span>国外客服</span>1234567\n                </p>\n              </div>\n            </div>\n          </div>\n        ) : (\n          \"\"\n        )}\n        <div className=\"ComponentRouter\">\n          <Button\n            type=\"text\"\n            size=\"large\"\n            className={this.state.componentNum == 0 ? \"clickEd\" : \"Noclick\"}\n            onClick={this.toHomeChildRouter.bind(this, \"0\")}\n          >\n            首页\n          </Button>\n          <Button\n            type=\"text\"\n            size=\"large\"\n            className={this.state.componentNum == 1 ? \"clickEd\" : \"Noclick\"}\n            onClick={this.toHomeChildRouter.bind(this, \"1\")}\n          >\n            机票\n          </Button>\n          <Button\n            type=\"text\"\n            size=\"large\"\n            className={this.state.componentNum == 2 ? \"clickEd\" : \"Noclick\"}\n            onClick={this.toHomeChildRouter.bind(this, \"2\")}\n          >\n            酒店\n          </Button>\n          <Button\n            type=\"text\"\n            size=\"large\"\n            className={this.state.componentNum == 3 ? \"clickEd\" : \"Noclick\"}\n            onClick={this.toHomeChildRouter.bind(this, \"3\")}\n          >\n            火车票\n          </Button>\n          <Button\n            type=\"text\"\n            size=\"large\"\n            className={this.state.componentNum == 4 ? \"clickEd\" : \"Noclick\"}\n            onClick={this.toHomeChildRouter.bind(this, \"4\")}\n          >\n            度假\n          </Button>\n          <Button\n            type=\"text\"\n            size=\"large\"\n            className={this.state.componentNum == 5 ? \"clickEd\" : \"Noclick\"}\n            onClick={this.toHomeChildRouter.bind(this, \"5\")}\n          >\n            团购\n          </Button>\n          <Button\n            type=\"text\"\n            size=\"large\"\n            className={this.state.componentNum == 6 ? \"clickEd\" : \"Noclick\"}\n            onClick={this.toHomeChildRouter.bind(this, \"6\")}\n          >\n            门票\n          </Button>\n          <Button\n            type=\"text\"\n            size=\"large\"\n            className={this.state.componentNum == 7 ? \"clickEd\" : \"Noclick\"}\n            onClick={this.toHomeChildRouter.bind(this, \"7\")}\n          >\n            当地人\n          </Button>\n        </div>\n        <Router>\n          <Switch>\n            <Route path=\"/main\" component={Main} />\n            <Route path=\"/planeTicket\" component={PlaneTicket} />\n            <Route path=\"/hotel\" component={Hotel} />\n            <Route path=\"/trainTickets\" component={TrainTickets} />\n            <Route path=\"/holiday\" component={Holiday} />\n            <Route path=\"/groupBuying\" component={GroupBuying} />\n            <Route path=\"/tickets\" component={Tickets} />\n            <Route path=\"/local\" component={Local} />\n            <Route path=\"/\" exact component={Main} />\n          </Switch>\n        </Router>\n        <div className=\"fixedFixed\">\n          <LeftFixed />\n        </div>\n      </div>\n    );\n  }\n}\nexport default withRouter(Home);\n","/Users/huanglin/Downloads/hlxll/react/react/school-demo/src/view/login/index.jsx",["206"],"import React, { Component } from \"react\";\nimport SmallLogin from \"../../component/login\";\nimport LoginHead from \"../../component/login/loginHead\";\nimport \"./index.less\";\nimport { Image, Select, Form, Button, Input } from \"antd\";\nimport VerficationCode from \"../../component/login/verificationCode\";\nconst { Option } = Select;\nclass Home extends Component {\n  constructor(props) {\n    super(props);\n    this.verfication = React.createRef();\n    this.state = {\n      loginOrRegister: true,\n    };\n    this.checkLoginRegister = this.checkLoginRegister.bind(this);\n    this.onFinish = this.onFinish.bind(this);\n    this.onFinishFailed = this.onFinishFailed.bind(this);\n  }\n  checkLoginRegister(data) {\n    this.setState({\n      loginOrRegister: data,\n    });\n  }\n  onFinish(e) {\n    console.log(this.verfication.current.state);\n    console.log(e);\n  }\n  onFinishFailed() {}\n  render() {\n    return (\n      <div className=\"loginMain\">\n        <div className=\"head\">\n          <LoginHead checkLoginRegister={this.checkLoginRegister} />\n        </div>\n        <div className=\"loginCenter\">\n          <div className=\"logImg\">\n            <Image\n              style={{ width: 150, height: 50 }}\n              src=\"https://gw.alipayobjects.com/zos/antfincdn/aPkFc8Sj7n/method-draw-image.svg\"\n            />\n            {this.state.loginOrRegister ? (\n              <span>账号登录</span>\n            ) : (\n              <span>账号注册</span>\n            )}\n          </div>\n          {this.state.loginOrRegister ? (\n            <div className=\"CenterMain\">\n              <div className=\"MainLeft\">\n                <Image\n                  width={200}\n                  src=\"https://zos.alipayobjects.com/rmsportal/jkjgkEfvpUPVyRjUImniVslZfWPnJuuZ.png\"\n                />\n              </div>\n              <SmallLogin />\n            </div>\n          ) : (\n            <>\n              <Form\n                labelCol={{\n                  span: 4,\n                }}\n                wrapperCol={{\n                  span: 14,\n                }}\n                layout=\"horizontal\"\n                onFinish={this.onFinish}\n                onFinishFailed={this.onFinishFailed}\n              >\n                <Form.Item\n                  label=\"国家/地区\"\n                  name=\"country\"\n                  style={{ width: 500 }}\n                  rules={[\n                    {\n                      required: true,\n                      message: \"Please input your telephone!\",\n                    },\n                  ]}\n                >\n                  <Select\n                    showSearch\n                    style={{ width: 200 }}\n                    placeholder=\"Select a person\"\n                    optionFilterProp=\"children\"\n                    filterOption={(input, option) =>\n                      option.children\n                        .toLowerCase()\n                        .indexOf(input.toLowerCase()) >= 0\n                    }\n                  >\n                    <Option value=\"jack\">Jack</Option>\n                    <Option value=\"lucy\">Lucy</Option>\n          