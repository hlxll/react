            <Form.Item\n                      name=\"start\"\n                      rules={[\n                        {\n                          required: true,\n                          message: \"Please select!\",\n                        },\n                      ]}\n                    >\n                      <Select\n                        showSearch\n                        style={{ width: 200 }}\n                        placeholder=\"从 国家/城市\"\n                        optionFilterProp=\"children\"\n                      >\n                        {this.state.chinaCity.map((item) => (\n                          <Option value={item}>{item}</Option>\n                        ))}\n                      </Select>\n                    </Form.Item>\n                  </Col>\n                  <Col span={4}>\n                    <Form.Item\n                      name=\"arriver\"\n                      rules={[\n                        {\n                          required: true,\n                          message: \"Please select!\",\n                        },\n                      ]}\n                    >\n                      <Select\n                        showSearch\n                        style={{ width: 200 }}\n                        placeholder=\"到 国家/城市\"\n                        optionFilterProp=\"children\"\n                      >\n                        {this.state.chinaCity.map((item) => (\n                          <Option value={item}>{item}</Option>\n                        ))}\n                      </Select>\n                    </Form.Item>\n                  </Col>\n                  <Col span={4}>\n                    <Form.Item\n                      name=\"startDay\"\n                      rules={[\n                        {\n                          required: true,\n                          message: \"Please select!\",\n                        },\n                      ]}\n                    >\n                      <DatePicker format={dateFormat} />\n                    </Form.Item>\n                  </Col>\n                  {this.state.moveNum == 1 ? (\n                    \"\"\n                  ) : (\n                    <Col span={4}>\n                      <Form.Item\n                        name=\"arriveDay\"\n                        rules={[\n                          {\n                            required: true,\n                            message: \"Please select!\",\n                          },\n                        ]}\n                      >\n                        <DatePicker format={dateFormat} />\n                      </Form.Item>\n                    </Col>\n                  )}\n\n                  <Col span={4}>\n                    <Form.Item>\n                      <Button\n                        type=\"primary\"\n                        htmlType=\"submit\"\n                        min-width=\"264px\"\n                        className=\"loginBtn\"\n                      >\n                        登录\n                      </Button>\n                    </Form.Item>\n                  </Col>\n                </Row>\n              </div>\n            </Form>\n          </div>\n        </div>\n        <div className=\"detailMain\">\n          {this.state.planeList.map((item) => (\n            <div className=\"MainItem\" onClick={this.pushDetail}>\n              <div className=\"planeLog\">\n                <Image src={item.src} />\n              </div>\n              <div className=\"MainLeftName\">\n                <span className=\"leftNameC\">{item.name}</span>\n                <div className=\"leftNameE\">{item.Ename}</div>\n              </div>\n              <div className=\"MainCenterTime\">\n                <div className=\"startEndTime\">\n                  <span className=\"SETime\">{item.startTime}</span>\n                  <p className=\"SECity\">{item.start}</p>\n                </div>\n                <div className=\"allTime\">\n                