ewArr.push(arr2[1] - arr1[1] + 60);\n      newArr.push(\"分钟\");\n    }\n    return newArr.join(\"\");\n  };\n  render() {\n    return (\n      <div id=\"detailTicket\">\n        <div className=\"detailHead\">\n          <div className=\"\">\n            <Form\n              name=\"basic\"\n              onFinish={this.onFinish}\n              onFinishFailed={this.onFinishFailed}\n            >\n              <Form.Item>\n                <Radio.Group\n                  value={this.state.moveNum}\n                  onChange={this.moveChange}\n                >\n                  <Radio value={1}>单程</Radio>\n                  <Radio value={2}>往返</Radio>\n                </Radio.Group>\n              </Form.Item>\n              <div className=\"startEndCityTime\">\n                <Row>\n                  <Col span={4}>\n                    <Form.Item\n                      name=\"start\"\n                      rules={[\n                        {\n                          required: true,\n                          message: \"Please select!\",\n                        },\n                      ]}\n                    >\n                      <Select\n                        showSearch\n                        style={{ width: 200 }}\n                        placeholder=\"从 国家/城市\"\n                        optionFilterProp=\"children\"\n                      >\n                        {this.state.chinaCity.map((item) => (\n                          <Option value={item}>{item}</Option>\n                        ))}\n                      </Select>\n                    </Form.Item>\n                  </Col>\n                  <Col span={4}>\n                    <Form.Item\n                      name=\"arriver\"\n                      rules={[\n                        {\n                          required: true,\n                          message: \"Please select!\",\n                        },\n                      ]}\n                    >\n                      <Select\n 