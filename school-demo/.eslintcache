tState({\n      isModalVisible: false,\n    });\n    this.searchUser();\n  }\n  handleCancel = () => {\n    this.setState({\n      isModalVisible: false,\n    });\n  };\n  userTypeChange(e) {\n    this.setState({\n      userTypeArr: e,\n    });\n  }\n  typeForEa(text) {\n    let res = \"\";\n    text.split(\",\").map((item) => {\n      let num = +item;\n      res += this.state.userType[num - 1];\n    });\n    return res;\n  }\n  render() {\n    const columns = [\n      {\n        title: \"用户名\",\n        dataIndex: \"username\",\n        key: \"username\",\n      },\n      {\n        title: \"人员类型\",\n        dataIndex: \"jurisdiction\",\n        key: \"jurisdiction\",\n        render: (text) => (\n          <div>\n            {text == \"admin\"\n              ? \"管理员\"\n              : text == \"ordinary\"\n              ? \"普通用户\"\n              : this.typeForEa(text)}\n          </div>\n        ),\n      },\n      {\n        title: \"头像\",\n        dataIndex: \"headImg\",\n        key: \"headImg\",\n        render: (text) => <Image width={100} height={100} src={text} />,\n      },\n      {\n        title: \"操作\",\n        dataIndex: \"username\",\n        key: \"username\",\n        render: (text, record) => (\n          <Space size=\"middle\">\n            <Button onClick={this.updateJurisdiction.bind(this, text)}>\n              权限修改\n            </Button>\n            <Button onClick={this.deleteUser.bind(this, text)}>删除用户</Button>\n          </Space>\n        ),\n      },\n    ];\n    const modalChildren = [];\n    for (let i = 1; i < 8; i++) {\n      modalChildren.push(<Option key={i}>{this.state.userType[i - 1]}</Option>);\n    }\n    return (\n      <div>\n        <Table columns={columns} dataSource={this.state.data} />\n        <Modal\n          title={this.state.modalName + \"权限修改\"}\n          visible={this.state.isModalVisible}\n          footer={false}\n          onCancel={this.handleCancel}\n        >\n          <Select\n            mode=\"multiple\"